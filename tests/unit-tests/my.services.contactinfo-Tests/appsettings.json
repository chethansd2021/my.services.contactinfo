{
  "ServiceSettings": {
    "name": "my-services-contactinfo",
    "Description": "my.services.contactinfo",
    "Instance": "my.services.contactinfo",
    "Version": "1.0"
  },

  "SwaggerSettings": {
    "JsonRoute": "swagger/{documentName}/swagger.json",
    "Description": "my.services.contactinfo",
    "UiEndpoint": "v1/swagger.json"
  },

  "DbConnectionSettings": {
    "FranForceConnectionString": "Server=127.0.0.1;Port=5432;Database=postgres;User Id=;Password=",
    "PsqlConnectionString": "Server=127.0.0.1;Port=5432;Database=postgres;User Id=;Password=",
    "DefaultTimeoutInSeconds": 30,
    "DisableAwsAuthenticationForPsql": true
  },

  "AwsConfig": {
    "Region": "us-east-2",
    "EventBus": "my-services-contactinfo-event-bus"
  },

  "LogSettings": {
    "applicationName": "my-services-contactinfo",
    "excludePaths": [ "/ping", "/metrics", "/health" ],
    "MinimumLevelOverrides": {
      "Microsoft": "Warning",
      "System": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    },
    "requestPayloadLevel": "Information",
    "level": "Debug",
    "console": {
      "enabled": true,
      "useSimpleFormat": false
    },
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "Day"
    }
  }
}
